[sub_resource type="GDScript" id="GDScript_mjl6i"]
script/source = "extends Control

@onready var grid := $GridContainer

var levels := [
	{ \"name\": \"Level 1\", \"scene\": \"res://levels/level1.tscn\" },
	{ \"name\": \"Level 2\", \"scene\": \"res://levels/level2.tscn\" },
	{ \"name\": \"Level 3\", \"scene\": \"res://levels/level3.tscn\" },
	{ \"name\": \"Level 4\", \"scene\": \"res://levels/level4.tscn\" },
	{ \"name\": \"Level 5\", \"scene\": \"res://levels/level5.tscn\" },
	{ \"name\": \"Level 6\", \"scene\": \"res://levels/levelMidBoss.tscn\" },
	{ \"name\": \"Level 7\", \"scene\": \"res://levels/level6.tscn\" },

	{ \"name\": \"Level 8\", \"scene\": \"res://levels/level7.tscn\" },
	{ \"name\": \"Level 9\", \"scene\": \"res://levels/level8.tscn\" },
	{ \"name\": \"Level 10\", \"scene\": \"res://levels/levelBossFinal.tscn\" },


]

func _ready() -> void:
	create_level_buttons()

func create_level_buttons() -> void:
	for level in levels:
		var btn := SelectButton.new()
		btn.text = level.name
		btn.scene = level.scene
		grid.add_child(btn)

func _on_close_texture_button_pressed() -> void:
	get_tree().change_scene_to_file(\"res://scenes/screens/main_screen.tscn\")


func _on_equip_texture_button_pressed() -> void:
	var choose_weapons = preload(\"res://scenes/ui/choose_weapons/choose_weapons.tscn\").instantiate()
	add_child(choose_weapons)
"

